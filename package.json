{
  "name": "jazdosanjh.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "node-sass": "^4.9.0"
  },
  "scripts": {
    "watch:sass": "node-sass assets/sass/main.scss assets/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass assets/sass/main.scss assets/css/style.comp.css",
    "concat:css": "concat -o assets/css/style.concat.css assets/css/style.comp.css",
    "post:css": "postcss assets/css/style.concat.css -u postcss-prefixer -o assets/css/style.prefix.css",
    "compress:css": "node-sass assets/css/style.prefix.css assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css post:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jazdosanjh/jazdosanjh.github.io.git"
  },
  "author": "JSD",
